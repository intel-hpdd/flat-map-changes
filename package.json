{
  "name": "@mfl/flat-map-changes",
  "version": "1.0.3",
  "description": "Highland.js flatMap transform that creates a new source when it's upstream changes.",
  "main": "dist/source/index.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t",
    "eslint": "eslint ./",
    "flow": "flow",
    "postversion": "./bin/build.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "eslint",
    "test:dev",
    "flow"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:MFL/flat-map-changes.git"
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-strict-mode": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "eslint": "3.9.1",
    "eslint-plugin-flowtype": "2.25.0",
    "eslint-plugin-import": "2.0.1",
    "flow-bin": "0.34.0",
    "@mfl/flow-highland": "3.9.0",
    "@mfl/flow-jasmine": "1.5.0",
    "@mfl/jasmine-junit-reporter": "1.0.0",
    "@mfl/jasmine-n-matchers": "2.1.1",
    "jasmine": "2.5.2",
    "nyc": "8.3.2",
    "pre-commit": "1.1.3"
  },
  "dependencies": {
    "highland": "3.0.0-beta.2",
    "@mfl/fp": "7.0.0"
  }
}
